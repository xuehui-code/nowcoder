import java.io.*;

public class Main{
     public static void main(String[] args)throws IOException{
         char s = (char) System.in.read();
         for(int i = 1;i<=5;i++){
             for(int j = 1;j<=5-i;j++){
                 System.out.print(" ");
             }
             for(int k =1;k<=i;k++){
                 System.out.print(s+" ");
             }
             System.out.print("\n");
         }
     }
}
